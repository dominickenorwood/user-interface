.calSub .disabled{opacity: .5}
.calculate ui-btn-hidden ui-disabled{opacity: .5}
input#calculate.calculate.ui-btn-hidden{opacity: .5}
.calSub .ui-btn-inner{padding:10px 20px;margin:0;}
.calSub .ui-btn-text{color:#305101;font-size:18px; line-height:24px;font-weight:normal;}
.calResults .calSub {margin-top:10px;border-top:none;padding-bottom:5px;}
.calResults .calSub .ui-btn{margin-top:10px;}
.calResults .resultsHead{background-color:#f2f2f2;padding:10px 15px 5px 15px;border-bottom:solid 1px #acb999;}
.calResults .box{padding:10px 15px 5px 15px;border-bottom:solid 1px #acb999;}
.calResults .formRow{clear:both;}
.calResults label{color:#305101;font-weight:bold;display:block;font-size:12px;line-height:18px;width:50%;float:left;margin:0;}
.calResults input.ui-input-text{width:45%;float:right;margin:0;border:none;background:none;font-size:12px;line-height:18px;color:#305101;font-weight:bold;}
.calResults .box input.ui-input-text{font-weight:normal;}
.monAccordian .calResults h4{color:#305101;font-size:14px;text-transform:uppercase;font-weight:bold;margin:0;padding:0 0 15px 0;line-height:20px;}
.monAccordian .calResults .calResultHead {padding:10px 15px;border-bottom:solid 2px #305101;}
.monAccordian .calResults .calResultHead h4{padding:0;}
.monAccordian .calResults table{width:100%;font-size:12px;line-height:18px;color:#305101;border-bottom:solid 1px #305101;}
.monAccordian .calResults table td{width:50%;vertical-align:top;border-right:solid 1px #acb999;text-align:left;}
.monAccordian .calResults table tr td:last-child{border-right:none;text-align:left;padding-left:10px;}
.monAccordian .calResults table tr td:first-child{font-weight:bold;border-right:solid 1px #acb999;text-align:left;}
.monAccordian .calResults table th{padding:5px 15px;text-align:left;text-transform:uppercase;vertical-align:top;border-right:solid 1px #acb999;border-bottom:solid 1px #acb999;}
.monAccordian .calResults table tr th:last-child{border-right:none;}
.monAccordian .calResults table tr.grey td{background-color:#f2f2f2;}

/*BOOKMARK//////////////////////////////////////////////*/
.monBookmark {border-top:solid 1px #b1b1b1;border-bottom:solid 1px #b1b1b1;margin:15px 0 0 0;}
.monBookmark a{
	display:block;
	color:#FFF;
	padding:10px;
	background: rgb(27,27,27); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(27,27,27,1) 1%, rgba(54,54,54,1) 63%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(27,27,27,1)), color-stop(63%,rgba(54,54,54,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(27,27,27,1) 1%,rgba(54,54,54,1) 63%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(27,27,27,1) 1%,rgba(54,54,54,1) 63%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(27,27,27,1) 1%,rgba(54,54,54,1) 63%); /* IE10+ */
	background: linear-gradient(top,  rgba(27,27,27,1) 1%,rgba(54,54,54,1) 63%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1b1b1b', endColorstr='#363636',GradientType=0 ); /* IE6-9 */
	}
.monBookmark span{float:right;font-size:14px;text-transform:uppercase;font-weight:bold;}
#page5 .monBookmark{margin:0;border-top:0;}
#page5 .content{background:none;border:none;-webkit-box-shadow:none;-moz-box-shadow:none;box-shadow:none;}
.monBookBox{padding:10px;color:#b3b2b2;}
.monBookBox p{padding:10px 0;}
.monBookBox ol{padding:0 20px;}
.monBookBox li{padding:4px 0;}

/*////////////////////////////////////////////////
ACCORDIAN
////////////////////////////////////////////////*/
.monAccordian{color:#595858;}
.monAcBox{margin:15px 0;border:solid 2px #FEFDFD;-webkit-box-shadow:2px 2px 10px #2c421d;-moz-box-shadow:2px 2px 10px #2c421d;box-shadow:2px 2px 10px #2c421d;background-color:#00853F;

}
.monAccordian .monActive{-webkit-border-bottom-right-radius: 3px;
-webkit-border-bottom-left-radius: 3px;
-moz-border-radius-bottomright: 3px;
-moz-border-radius-bottomleft: 3px;
border-bottom-right-radius: 3px;
border-bottom-left-radius: 3px;}

.monAccordian h1 a{
	display:block;
	font-size:14px;
	line-height:20px;
	text-transform:uppercase;
	color:#FFF;
	font-weight:normal;
	padding:10px 15px;
	position:relative;
	z-index:1;
	-webkit-box-shadow:inset 0 0 10px #434343;-moz-box-shadow:inset 0 0 10px #434343;box-shadow:inset 0 0 10px #434343;
	}
.monAccordian h1 a.monCurrent{}
.monAcCon{background-color:#ffffff;border-top:solid 1px #999999;}
.monAccordian .monAcInner{padding:5px 15px;border-bottom:solid 1px #a1af8f;}
.monAccordian .monAcInner:last-child{border:none;}
.monAccordian .monBox {padding:5px 15px;}
.monAccordian .monInBox{padding:10px 30px;}
.monAccordian .monInBox h1{padding-top:10px;}
.monAccordian .monBox h1{padding-top:10px;}
.monAccordian .monAcInner .monInBox{padding:0 20px;}
.monAccordian .monBox h1{font-size:18px;line-height:24px;color:#595858;padding-bottom:10px;}
.monAccordian h2{padding:8px 15px;font-size:16px;line-height:22px;color:#305101;border-bottom:solid 1px #acb999;border-top:solid 1px #acb999;}
.monAccordian h2:first-child{border-top:none;}
.monAccordian h3 {font-size:16px;line-height:22px;}
.monAccordian h3 a{display:block;padding:8px 40px 8px 15px;font-size:16px;line-height:22px;color:#00652f;font-weight:normal;border-bottom:solid 1px #acb999;background:none; position:relative;}
.monAccordian h3 a.monCurrent{font-weight:bold;padding:8px 40px 8px 15px;}
.monAccordian h3 a span{position:absolute; right:15px;top:8px;}
.monAccordian ul{margin:10px 20px;}
.monAccordian ol{margin:10px 20px;}
.monAccordian li{font-size:14px;line-height:20px;padding:5px 0;}
.monAccordian .legal{background-color:#f2f2f2;padding:10px 15px;border-top:solid 1px #acb999;}
.monAccordian .legal ol{margin:0 15px;}
.monAccordian .legal li{font-size:12px;line-height:18px;padding:5px 0;}
.monAccordian .legal .smallTxt {margin-top:10px;}
.monAccordian .legal .smallTxt:last-child{margin:0;}
.monAccordian .monInBox ul {margin:0;}
.monAccordian .monInBox li{list-style-type:none;display:block;color:#305101;font-weight:bold}
.monAccordian .monInBox li span{float:right;}
.monAccordian h4{font-size:14px;padding:5px 0;color:#595858;}
.monAccordian h5{color:#305101;font-size:18px;line-height:25px;padding:2px 0;}
.monAccordian h6{color:#595858;font-size:12px;font-style:italic;font-weight:normal;}
.monAccordian a{color:#305101;}
.monAccordian p{color:#595858;padding:10px 0;}
.monAccordian p.smallTxt{font-size:13px;line-height:19px;}
.monAccordian p span{color:#305101;}
.monAccordian p span.monBig{color:#305101;font-weight:bold;font-size:20px;line-height:26px;}
.monAccordian p.address{text-transform:uppercase;}
.monAccordian p.address span{font-weight:bold;}
.monAccordian p.address .number{font-weight:normal;color:#595858;text-transform:none;}
.monAccordian p.address .number span{font-weight:normal;}
.monAccordian span{color:#305101;}
.monAccordian span span{font-size:17px;font-weight:normal;}
.monAccordian table{width:100%;color:#666666;}
.monAccordian table h3{color:#305101;}
.monAccordian table td{border-bottom:solid 1px #acb999;text-align:center;padding:5px;width:30%;}
.monAccordian table tr:last-child td{border:none;}
.monAccordian table tr td:last-child{padding-right:15px;text-align:right;}
.monAccordian table tr td:first-child{text-align:left;padding-left:15px;}
.monAccordian table tr.bkg{background-color:#f2f2f2;}
.monAccordian table span{display:block;width:9px;height:8px;float:right;}
.monAccordian table span.down{background:url(/work/deltapine_mobile/images/downUp.png) no-repeat left bottom;}
.monAccordian table span.up{background:url(/work/deltapine_mobile/images/downUp.png) no-repeat left top;}
.monAccordian .monTxtForm table{margin-top:-15px;}
.monAccordian .monTxtForm td{width:80%;padding-left:20px;vertical-align:top;}
.monAccordian .monTxtForm tr td:first-child{width:20%;padding-left:0;}
.monAccordian .monTxtForm table tr td:last-child{padding-right:1px;text-align:left;}
.monAccordian table.commTable tr td:last-child div{width:35px;}
