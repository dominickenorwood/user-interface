/*////////////////////////////////////////////////
TEMPLATE
////////////////////////////////////////////////*/
*{margin:0;padding:0;}
a{outline:none;text-decoration:none;}
img{border:none;}
*:focus{outline:none;}
html{font-family:Arial, Helvetica, sans-serif;font-size:16px;line-height:22px;}
body{ background-color:#272727;}
table{border-collapse:collapse;width:100%;}
input{border:solid 1px #bcbcba;}
.monWrapper{background-color:#7AC143;-webkit-box-shadow:inset 0 5px 25px #3c5e21;}
/*input.error{border:solid 3px #F00;}*/ /*ERROR STYLE*/

/*////////////////////////////////////////////////
HEADER
////////////////////////////////////////////////*/
header{width:100%;height:107px;background:url(/work/deltapine_mobile/images/header_bkg.png) repeat-x left top; position:relative;z-index:2;border-bottom:solid 2px #FFF;}
header div{width:100%;height:106px;border-bottom:solid 1px #d1ded8;text-align:center;}
header img{margin-top:5px;}

/*////////////////////////////////////////////////
NAVIGATION
////////////////////////////////////////////////*/
nav{}
nav a{
	color:#FFF;
	text-transform:uppercase;
	font-weight:bold;
	font-size:18px;
	background-color:#278755;
	text-shadow: 2px 2px #185334;
	-webkit-box-shadow:inset 0 0 40px #141414;-moz-box-shadow:inset 0 0 40px #141414;box-shadow:inset 0 0 40px #141414;
}
nav a.monCurrent{
	background-color:transparent;
	-webkit-box-shadow:none;
	-moz-box-shadow:none;
	box-shadow:none;
	text-shadow: 2px 2px #4D792A;
	}
nav .ui-btn-text{font-size:16px;line-height:24px; }
nav .ui-btn{height:40px;}

/*////////////////////////////////////////////////
CONTENT
////////////////////////////////////////////////*/
.content{border-bottom:solid 1px #999;}
.monContent{padding:10px;}
.bottomLogo{background-color:#f2f2f2;padding:10px 0;width:100%;text-align:center;}
.monWeather{padding:5px;text-align:right;color:#FFF;font-size:13px;line-height:19px;margin:5px 0 10px 0;border:solid 2px #FEFDFD;-webkit-box-shadow:2px 2px 10px #2c421d;-moz-box-shadow:2px 2px 10px #2c421d;box-shadow:2px 2px 10px #2c421d;background-color:#00853F;}
.monWeather td {padding-left:15px;}
.monWeather td.left{width:60%;}
.monWeather td.left span{text-transform:uppercase;}
.monWeather td.middle{width:10%;font-size:35px;line-height:35px; font-weight:bold;}
.monWeather td.middle span{font-weight:normal;}
.monWeather td.right{width:30%;padding-left:10px;}
.monWeather td.right a{
	display:block;
	text-align:center;
	color:#FFF;
	padding:5px;
	font-size:12px;
	line-height:16px;
	text-transform:uppercase;
	background-color:#00652f;
	}

/*HOME//////////////////////////////////////////////*/
#page1 .content{background:url(/work/deltapine_mobile/images/home_bkg.png) no-repeat left top;width:100%;background-size: 100% 100%;}
#page1 form{margin:70% 10px 30% 10px;}
#page1 .zipBkg{background-color:#00853F;border:solid 2px #FEFDFD;-webkit-box-shadow:2px 2px 20px #000;-moz-box-shadow:2px 2px 20px #000;box-shadow:2px 2px 20px #000;}
#page1 label{color:#FFF;font-size:14px;line-height:20px;text-align:right;display:block;}
#page1 table{-webkit-box-shadow:inset 0 0 10px #434343;-moz-box-shadow:inset 0 0 10px #434343;box-shadow:inset 0 0 10px #434343;}
#page1 td{padding:5px 10px;}
#page1 td:first-child{padding-right:0;}
#page1 td.monHomeZip{width:45%;}
#page1 form p{text-align:right;color:#FFF;padding:5px 0;}
#page1 .ui-btn { margin:0; background-color:#7AC143;color:#FFF;font-size:22px;line-height:22px;-webkit-border-radius: 30px;-moz-border-radius: 30px;border-radius: 30px; height:40px;width:40px;}
#page1 .ui-btn-inner {  padding:8px 12px 5px 12px;}

/*////////////////////////////////////////////////
TEXT ALERT FORM
////////////////////////////////////////////////*/
.monTxtForm{margin-top:-10px;}
.monTxtForm div{margin:10px 0;}
.monTxtForm div.zipNum{margin-top:-10px;}
.monTxtForm div.pNumber{width:70%;float:right;}
.monTxtForm div.zipCode{width:20%;float:left;}
.monTxtForm td{/*width:80%;*/padding-left:20px;vertical-align:top;}
.monTxtForm tr td:first-child{/*width:20%;*/padding-left:0;}
.monTxtForm tr td:first-child{background-color:#093;width:200px;}
.monTxtForm label{color:#305101;font-weight:bold;display:block;margin:0 0 5px 0;font-size:12px;line-height:18px;}
.monTxtForm input.ui-input-text{background-color:#e0e0e0;border:solid 1px #adadad;
-webkit-box-shadow:inset -2px -2px 5px #a0a09e;-moz-box-shadow:inset -2px -2px 5px #a0a09e;box-shadow:inset -2px -2px 5px #a0a09e;}
.monAccordian .monTxtForm p{font-size:12px; line-height:18px;padding:0;display:block;float:left;width:85%}
.monTxtForm .ui-checkbox {margin:0 15px 0 0;padding:0;float:left;width:5%;}
.monTxtForm .ui-checkbox input{padding:0;margin:0;position:static;width:20px;height:20px;}

.monTxtForm .monTxtSub {margin-top:0;}
.monTxtForm .monTxtSub .ui-btn{
	margin-top:20px;
	padding:0;
	float:right;
	border:solid 1px #00853f;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	background: rgb(174,230,131); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(174,230,131,1) 46%, rgba(122,193,67,1) 54%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(46%,rgba(174,230,131,1)), color-stop(54%,rgba(122,193,67,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(174,230,131,1) 46%,rgba(122,193,67,1) 54%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(174,230,131,1) 46%,rgba(122,193,67,1) 54%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(174,230,131,1) 46%,rgba(122,193,67,1) 54%); /* IE10+ */
	background: linear-gradient(top,  rgba(174,230,131,1) 46%,rgba(122,193,67,1) 54%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#aee683', endColorstr='#7ac143',GradientType=0 ); /* IE6-9 */
	}
.monTxtForm .monTxtSub .ui-btn-inner{padding:10px 20px;margin:0;}
.monTxtForm .monTxtSub .ui-btn-text{color:#305101;font-size:18px; line-height:24px;font-weight:normal;}
.monTxtForm .ui-select .ui-btn{padding:0;}
.monTxtForm .ui-select .ui-btn-icon-right{padding:0;}
.monTxtForm .ui-select{padding:0;}
.monTxtForm .ui-select select{padding:0;}
.monTxtForm .ui-select .ui-icon{
	padding:10px;
	right:0;
	top:9px;
	background-color:#F00;
	border-left:solid 1px #4c4c4c;
	-webkit-border-top-right-radius: 10px;
	-webkit-border-bottom-right-radius: 10px;
	-moz-border-radius-topright: 10px;
	-moz-border-radius-bottomright: 10px;
	border-top-right-radius: 10px;
	border-bottom-right-radius: 10px;
	background: rgb(162,162,162); /* Old browsers */
	background: url(/work/deltapine_mobile/images/drop_arrow.png) no-repeat 50% 50%, -moz-linear-gradient(top,  rgba(162,162,162,1) 0%, rgba(105,105,105,1) 100%); /* FF3.6+ */
	background:url(/work/deltapine_mobile/images/drop_arrow.png) no-repeat 50% 50%, -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(162,162,162,1)), color-stop(100%,rgba(105,105,105,1))); /* Chrome,Safari4+ */
	background:url(/work/deltapine_mobile/images/drop_arrow.png) no-repeat 50% 50%, -webkit-linear-gradient(top,  rgba(162,162,162,1) 0%,rgba(105,105,105,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(162,162,162,1) 0%,rgba(105,105,105,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(162,162,162,1) 0%,rgba(105,105,105,1) 100%); /* IE10+ */
	background: linear-gradient(top,  rgba(162,162,162,1) 0%,rgba(105,105,105,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#a2a2a2', endColorstr='#696969',GradientType=0 ); /* IE6-9 */
	}
.monTxtForm .ui-select .ui-btn-text{text-align:left;font-size:16px;line-height:16px;padding:10px;}
.monTxtForm .ui-select .ui-btn-inner{padding:0;margin:0;}
.monTxtForm .ui-select .ui-btn-corner-all{padding:0;margin:0;}
.monTxtForm .ui-select{
	width:75%;
	margin:20px 0;
	border:solid 1px #4c4c4c;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: rgb(254,254,254); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(254,254,254,1) 0%, rgba(207,207,207,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(254,254,254,1)), color-stop(100%,rgba(207,207,207,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(254,254,254,1) 0%,rgba(207,207,207,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(254,254,254,1) 0%,rgba(207,207,207,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(254,254,254,1) 0%,rgba(207,207,207,1) 100%); /* IE10+ */
	background: linear-gradient(top,  rgba(254,254,254,1) 0%,rgba(207,207,207,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#cfcfcf',GradientType=0 ); /* IE6-9 */
}

/*////////////////////////////////////////////////
CALCULATORS
////////////////////////////////////////////////*/
.calculator{}
.calculator .progress{text-align:right;padding-top:0;margin-top:-10px;display:none;}
.calculator .formRow{clear:both;}
.calculator .required{color:#F00;}
.calculator .reqTxt{text-align:right;margin-top:-10px;}
.calculator label{color:#305101;font-weight:bold;display:block;margin:15px 0 5px 0;font-size:12px;line-height:18px;width:50%;float:left;}
.calculator label span{color:#F00;}
.calculator input.ui-input-text{background-color:#e0e0e0;border:solid 1px #adadad;
-webkit-box-shadow:inset -2px -2px 5px #a0a09e;-moz-box-shadow:inset -2px -2px 5px #a0a09e;box-shadow:inset -2px -2px 5px #a0a09e;width:45%;float:right;margin-top:6px;margin-bottom:10px; }
.calculator .ui-select .ui-btn{padding:0;}
.calculator .ui-select .ui-btn-icon-right{padding:0;}
.calculator .ui-select{padding:0;}
.calculator .ui-select select{padding:0;}
.calculator .ui-select .ui-icon{
	padding:10px;
	right:0;
	top:6px;
	background: url(/work/deltapine_mobile/images/drop_arrow_green.png) no-repeat 50% 50%;
	}
