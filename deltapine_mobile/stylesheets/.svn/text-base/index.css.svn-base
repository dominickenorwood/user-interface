.monAccordian .monSearchBox{
	border-top:solid 1px #68705d;
	padding:10px 15px;
	font-size:13px;
	line-height:19px;
	color:#305101;
	}
.monSearchBox input.ui-input-text{ background-image: none;padding:0; width:100%;padding:5px;background-color:#e0e0e0;
-webkit-box-shadow:inset -2px -2px 5px #a0a09e;-moz-box-shadow:inset -2px -2px 5px #a0a09e;box-shadow:inset -2px -2px 5px #a0a09e; }
.monSearchBox table td{vertical-align:top;padding:0;}
.monSearchBox table td.left{width:40%;padding-top:6px;}
.monSearchBox table td.middle{width:55%;padding-left:10px;}
.monSearchBox table td.right{width:5%; padding-top:7px;}
.monSearchBox p{color:#F00;padding-top:2px;font-size:10px;line-height:16px;font-weight:bold;}
.monSearchBox table span{float:none;background:none;display:block;width:100%;height:100%;margin-left:15px;}
.monSearchBox label{font-weight:bold;text-align:right;display:block;color:#305101;font-size:12px;line-height:18px;padding-top:2px; white-space:pre;}

/*////////////////////////////////////////////////
FOOTER
////////////////////////////////////////////////*/
footer{width:100%;padding:20px 5px 50px 5px;}
footer a{color:#B2B2B2;font-size:12px;line-height:18px; display:block;}
footer li{float:left; list-style-type:none;padding:0 5px; margin-bottom:10px;}
footer li:last-child{padding-right:0;}
.monFootMenu li{border-right:solid 1px #B2B2B2;}
.monFootMenu li.last{border:none;}
.monFootMenu li a{line-height:12px;}
.monFacebook{margin-top:5px;}
.monFacebook a{padding:5px 0 5px 30px;background:url(/work/deltapine_mobile/images/footer_facebook_icon.gif) no-repeat left 50%;}
.monFacebook li:last-child a{ margin:0;}

